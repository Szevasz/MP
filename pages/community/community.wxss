  }
.TextBox{
    position: relative;
    width: 600rpx;
    background-color:rgba(138, 137, 124, 1);
    margin-top: 20rpx;
    border-radius: 20rpx;
}
.Text{
    position: relative;
    left: 10rpx;
    width:550rpx;
}
.Delete-Box{
    display: flex;
    justify-content: center;
}
.Delete{
    color: gray;
}
.LikeAndComment{
    display: flex;
    flex-direction: row;
    position: relative;
    margin-top: 10rpx;
    width: 600rpx;

}
.Like{
    position: relative;
    width: 60rpx;
    height: 60rpx;
    background:#657065;
    /* border-radius: 20rpx; */
}
.Comment{
    position: relative;
    width: 60rpx;
    height: 60rpx;
    left: 490rpx;
    background:#657065;
    /* border-radius: 20rpx; */
}

.likeicon{
    width: 60rpx;
    height: 60rpx;
}
.commenticon{
    width: 54rpx;
    height: 54rpx;
}
.Like-Box{
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
    background-color: #5b6456;
    position: relative;
    margin:0rpx 60rpx;
    border-radius: 20rpx;
}
.like_userhead{
    width: 60rpx;
    height: 60rpx;
    border-radius: 30rpx;
    margin: 5rpx 8rpx;
}
.Comcent-Box{
    background-color: #5b6456;
    padding: 20rpx 20rpx;
    margin: 0rpx 60rpx;
    margin-top: 15rpx;
    border-radius: 40rpx;
    margin-bottom: 15rpx;
}
.comcent{
    display: flex;
    flex-direction: row;
    margin-bottom: 10rpx;
}
.comcent-userhead{
    width: 80rpx;
    height: 80rpx;
    margin-right: 10rpx;
    border-radius: 10rpx;
}
.comcent-nameAndtime{
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    width: 500rpx;
}
.comcent-nickname{
    font-weight: bolder;
}
.comcent-time{
    color: #343a40;
    font-size: 24rpx;
}
.comment-obj{
    font-weight: bolder;
}
.pub-comment{
    display: flex;
    flex-direction: row;
    background-color: rgba(138, 137, 124, 1);
    padding: 15rpx 20rpx;
    align-items: center;
    position: fixed;
    bottom: 0;
}
.pub-left{
    color:#7f7f7f;
    background-color: rgba(189, 187, 176, 1);
    border-radius: 15rpx;
    margin-right: 30rpx;
}
.pub-input{
    padding: 10rpx 20rpx;
    width: 560rpx;
}
.pub-botton{
    border:solid 3rpx black;
    border-radius: 10rpx;
    padding: 10rpx 10rpx;
}
.blank{
    background-color: #657065;   
}
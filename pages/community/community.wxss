page{
    background-color: #657065;
    
}
.Add{
    position: absolute;
    display: flex;
    width: 100rpx;
    height: 100rpx;
    top: 15rpx;
    left: 640rpx;
    border-radius: 50rpx;
    background:rgba(189, 187, 176, 1);
    position: fixed;
    z-index: 99;
}
.Vertical{
    position: relative;
    height: 60rpx;
    width: 10rpx;
    background: rgb(64, 61, 57);
    left: -13rpx;
    top: 20rpx;
    border-radius: 20rpx;
}
.Transverse{
    height: 10rpx;
    width:60rpx;
    background: rgb(64, 61, 57);
    position: relative;
    top: 45rpx;
    left: 23rpx;
    border-radius: 20rpx;
}
.message{
    display: flex;
    flex-direction: column;
    justify-items: center;
    align-items: center;
    margin-top: 40rpx;
    margin-left: 5%;
    max-height: initial;
    width: 90%;
    border-radius: 10px;
    box-shadow: 0px 2px 4px 0px 
}
.InfoAndTime{
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-content: center;
    /* top: 40rpx; */
    height: 140rpx;
    width: 100%;
}
.Head{
    display: flex;
    position: relative;
    left:-60rpx;
    top: 35rpx;
    height: 100rpx;
    width: 100rpx;
}
.userHead
{
    height:100rpx;
    width:100rpx;
    border-radius: 50rpx;
    top: 20rpx;
}
.Name{
    display: flex;
    align-items: center;
    position: relative;
    left: -20rpx;
    top: 30rpx;
    height:100rpx;

}
.Time{
    display: flex;
    align-items: center;
